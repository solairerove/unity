    "angular-sanitize": "1.5.8",
    "angular-messages": "1.5.8",
    "angular-animate": "1.5.8",
    "bootstrap-material-design": "0.5.9",
    "angular-bootstrap-material": "abm#^0.1.4",
    "angular-bootstrap": "2.2.0",
    "bootstrap": "3.3.7",
    "ng-flow": "~2",
    "flow.js": "~2",
